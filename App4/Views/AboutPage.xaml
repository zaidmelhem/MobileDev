<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App4.Views.AboutPage"
             xmlns:vm="clr-namespace:App4.ViewModels"
             Title="{Binding Title}">
    <StackLayout >
        <StackLayout Orientation="Horizontal">
            <Button Text="Zajel" BackgroundColor="#005e66" Margin="20,20,50,30" Clicked="Button_Clicked"/>
            <Button Text="Google" BackgroundColor="Blue" Margin="80,20,50,30" Clicked="Button_Clicked_1"/>
        </StackLayout>
        <WebView x:Name="MyWebView" Source="https://najah.edu/" WidthRequest="250" HeightRequest="840" />
        
    </StackLayout>

</ContentPage>
